<div id="myCarousel" class="carousel slide carousel-fade" data-ride="carousel" data-interval="3000">
    <div id="close" class="close_cursor"> X </div>
        <ol class="carousel-indicators">
            {% for img in images %}
                <li data-target="#myCarousel" data-slide-to="{{ img.id }}" 
                    {% if img.id == first.id %}class="active"{% endif %}></li>
            {% endfor %}
        </ol>

        <div class="carousel-inner">
            {% for img in images %}
                <div class="item {% if img.id == first.id %}active{% endif %}">
                  <img src="{{ asset(img.webPath) }}" alt="{{ img.alt }}">

                </div>
          {% endfor %}
        </div>

        <a class="left carousel-control" href="#myCarousel" data-slide="prev">
          <span class="glyphicon glyphicon-chevron-left"></span>
          <span class="sr-only">Previous</span>
        </a>
        <a class="right carousel-control" href="#myCarousel" data-slide="next">
          <span class="glyphicon glyphicon-chevron-right"></span>
          <span class="sr-only">Next</span>
        </a>
        
</div>